{
  "name": "hud-disaster-data",
  "version": "1.0.0",
  "description": "FEMA needs to share post-disaster aid data with HUD and its grantees. This is a website that will facilitate that transfer of data.",
  "main": "index.js",
  "scripts": {
    "dev": "echo 'Run the client side hot reloading dev server' && node build/dev-server.js",
    "start": "echo 'Start the server' && node index",
    "start-cluster": "echo 'Start the server with pm2 cluster' && pm2 start index.js --name=\"DRDP\" -i max",
    "stop-cluster": "echo 'Stop the pm2 server cluster' && pm2 stop DRDP",
    "build": "echo 'Run the tests, build the webpack bundle, and test for 508 compliance.' && npm test && node build/build.js && npm run 508-test",
    "unit": "echo 'Run the client side tests' && cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "server-unit": "echo 'Run the server side tests' && mocha test/server && mocha test",
    "test": "echo 'Run all tests' && npm run unit && nyc --reporter=lcov --reporter=text --reporter=html npm run server-unit",
    "lint": "echo 'Run the linter' && eslint --ext .js,.vue src test/unit/specs",
    "508-test": "echo 'Run the 508 compliance tests' && bash ./508_Test.sh",
    "doc": "echo 'Run jsdoc and produce documentation' && jsdoc src build package.json -r -c jsdoc.conf.json -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flexion/hud-disaster-data.git"
  },
  "keywords": [
    "disaster",
    "FEMA",
    "HUD",
    "grant"
  ],
  "author": "18f/Flexion",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/flexion/hud-disaster-data/issues"
  },
  "homepage": "https://github.com/flexion/hud-disaster-data#readme",
  "nyc": {
    "exclude": [
      "test",
      "coverage"
    ]
  },
  "standard": {
    "globals": [
      "confirm",
      "Event",
      "describe",
      "it",
      "expect",
      "beforeEach",
      "localStorage"
    ]
  },
