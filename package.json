{
  "name": "hud-disaster-data",
  "version": "1.0.0",
  "description": "FEMA needs to share post-disaster aid data with HUD and its grantees. This is a website that will facilitate that transfer of data.",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "nyc --reporter=lcov --reporter=text --reporter=html mocha",
    "pack": "webpack ./lib/browser/index.js ./dist/js/bundle.js",
    "build": "npm run pack && npm run build-css && npm test",
    "build-css": "node-sass --include-path scss public/assets/_scss/app.scss public/assets/css/app.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flexion/hud-disaster-data.git"
  },
  "keywords": [
    "disaster",
    "FEMA",
    "HUD",
    "grant"
  ],
  "author": "18f/Flexion",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/flexion/hud-disaster-data/issues"
  },
  "homepage": "https://github.com/flexion/hud-disaster-data#readme",
  "nyc": {
    "exclude": [
      "test",
      "coverage"
    ]
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-session": "^2.0.0-beta.1",
    "express": "^4.15.2",
    "express-flash": "0.0.2",
    "lodash": "^4.17.4",
    "lusca": "^1.4.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-rc.1"
  },
  "devDependencies": {
    "cheerio": "^0.22.0",
    "jquery": "^3.2.1",
    "mocha": "^3.3.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "nyc": "^10.2.0",
    "should": "^11.2.1",
    "sinon": "^2.2.0",
    "supertest": "^3.0.0",
    "webpack": "^2.5.1"
  }
}
