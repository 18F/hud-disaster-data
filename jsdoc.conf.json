{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["lib"],
        "includePattern": "\\.[vue|js]",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["node_modules/jsdoc-vue"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
